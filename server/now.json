{
	"name": "node-lambda",
	"version": 2,
	"builds": [
		{ 
		    "src":  "index.js", 
	        "use": "@now/node-server",
	        "config": {
	        	"includeFiles": [
	        		"data/**"
	        	]
	        } 
        }       
	],
	"routes": [		
		{ "src": "/users", "methods": ["POST"],  "dest": "/index.js" },
		{ "src": "/user", "methods": ["POST"],  "dest": "/index.js" }		
	]
}